{
    "name": "ervinszabo/cakephp3-bootstrap-helpers",
    "description": "Bootstrap Helpers for CakePHP 3.0",
    "keywords": ["CakePHP", "Bootstrap"],
    "license": "Apache",
    "type": "cakephp-plugin",
    "repositories": [  
        {  
            "type": "vcs",  
            "url": "https://github.com/ervinszabo/cakephp-bootstrap-helpers"  
        }  
    ],  
    "require": {
        "cakephp/cakephp": ">=3.2.3"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "cakephp/cakephp-codesniffer": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Bootstrap\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bootstrap\\Test\\": "tests"
        }
    },
    "extra": {
        "installer-name": "Bootstrap"
    }
}
